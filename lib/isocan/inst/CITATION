citHeader("To cite isocan in publications use:")

citEntry(entry = "Article",
  title = "foo",
  author = personList(as.person("Shinichi Namba"),
  as.person("Masato Kawazu")),
  journal = "bar",
  year = "2018",
  volume = "1",
  number = "1",
  pages = "1-1",
  url = "baz",

  textVersion =
  paste("foo, bar, baz"))
